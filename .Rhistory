separateMessage <- function(msg){
#payload
payload = msg[c(-1:-4,(-length(msg)+2):(-length(msg)))]
payload_dataframe <- data.frame(
name = c()
hexCode = c()
)
indexPayload = 1
field = c(1,1,1,2,4,2,1,1,1)
for(i in field){
hex = ""
for(1:i){
hex = paste(hex, payload[indexPayload])
indexPayload = indexPayload + 1
}
payload_dataframe$hexCode.append(hex)
}
}
